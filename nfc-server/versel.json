{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/update", "methods": ["POST"], "dest": "/api/update.js" },
      { "src": "/api/data", "methods": ["GET"], "dest": "/api/data.js" }
    ]
  }